{
  "name": "and-hosted-maps",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "c8 tap",
    "start": "node --loader=@esbuild-kit/esm-loader src/index.ts",
    "dev": "nodemon --exec \"node --loader=@esbuild-kit/esm-loader\" src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Wolfgang Hobmaier <wolfgang.hobmaier@and-solution.com>",
  "license": "UNLICENSED",
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--loader=@esbuild-kit/esm-loader"
    ],
    "no-coverage-report": true,
    "test-env": [
      "NODE_ENV=test"
    ],
    "files": [
      "test/**/*.spec.ts"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.ts": [
      "yarn run lint --fix"
    ]
  },
  "dependencies": {
    "@esbuild-kit/esm-loader": "^2.4.2",
    "@fastify/type-provider-typebox": "^2.3.0",
    "@sinclair/typebox": "^0.24.41",
    "close-with-grace": "^1.1.0",
    "fastify": "^4.5.3",
    "fastify-multipart": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.17",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "tap": "^16.3.0",
    "typescript": "^4.8.3"
  }
}
