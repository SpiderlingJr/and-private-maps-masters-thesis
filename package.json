{
  "name": "and-hosted-maps",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "STRATEGY=redis c8 tap",
    "test-inmemory": "STRATEGY=memory c8 tap",
    "start": "node --loader=@esbuild-kit/esm-loader src/index.ts",
    "dev": "export $(cat .env); STRATEGY=redis DEBUG='info' nodemon --exec \"node --loader=@esbuild-kit/esm-loader\" src/index.ts",
    "dev-redis": "export $(cat .env); STRATEGY=redis nodemon --exec \"node --loader=@esbuild-kit/esm-loader\" src/index.ts",
    "dev-inmemory": "export $(cat .env); STRATEGY=memory nodemon --exec \"node --loader=@esbuild-kit/esm-loader\" src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Wolfgang Hobmaier <wolfgang.hobmaier@and-solution.com>",
  "license": "UNLICENSED",
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--loader=@esbuild-kit/esm-loader"
    ],
    "no-coverage-report": true,
    "test-env": [
      "NODE_ENV=test"
    ],
    "files": [
      "test/**/*.spec.ts"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.ts": [
      "yarn run lint --fix"
    ]
  },
  "dependencies": {
    "@esbuild-kit/esm-loader": "^2.5.4",
    "@fastify/autoload": "^5.7.1",
    "@fastify/formbody": "^7.2.0",
    "@fastify/multipart": "^7.1.2",
    "@fastify/postgres": "^5.2.0",
    "@fastify/redis": "^6.1.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@prisma/client": "4.9.0",
    "@sinclair/typebox": "^0.25.21",
    "@turf/turf": "^6.5.0",
    "@types/form-data": "^2.5.0",
    "@types/geojson": "^7946.0.10",
    "@types/jsonstream": "^0.8.30",
    "@types/ol": "^6.5.3",
    "@types/pg": "^8.6.5",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/pg-pool": "^2.0.3",
    "@types/redis": "^4.0.11",
    "@types/wellknown": "^0.5.3",
    "ajv": "^8.11.2",
    "close-with-grace": "^1.1.0",
    "esbuild": "^0.16.1",
    "fastify": "^4.12.0",
    "fastify-multipart": "^5.4.0",
    "fastify-plugin": "^4.2.1",
    "form-data": "^4.0.0",
    "geojson": "^0.5.0",
    "json5": "^2.2.3",
    "npm": "^9.4.1",
    "ol": "^7.1.0",
    "pg": "^8.9.0",
    "pg-copy-streams": "^6.0.4",
    "pg-pool": "^3.5.2",
    "redis": "^4.5.1",
    "tsc": "^2.0.4",
    "typeorm": "^0.3.10",
    "util.promisify": "^1.1.1",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/readable-stream": "^2.3.15",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@typescript-eslint/typescript-estree": "^5.45.1",
    "c8": "^7.12.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.8.0",
    "tap": "^16.3.4",
    "typescript": "^4.9.3"
  }
}
